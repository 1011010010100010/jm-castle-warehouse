{
  "name": "jm-castle-warehouse",
  "version": "1.0.0",
  "type": "module",
  "module": "./build",
  "private": false,
  "scripts": {
    "clean": "del-cli build",
    "prebuild": "npm run clean && eslint \"./src/**\"",
    "build": "tsc --project tsconfig.json",
    "start": "node ./build/start.mjs",
    "start-dev": "node ./build/start-dev.mjs",
    "dev": "nodemon --ext '*.mts' -x \"npm run build && npm run start-dev \"",
    "dev:serve": "cross-env NODE_ENV=development ts-node-dev --inspect=9229 --nolazy --respawn ./src -r ts-node/register src/."
  },
  "dependencies": {
    "jm-castle-warehouse-client": "^0.9.9",
    "jm-castle-warehouse-server": "^0.9.8",
    "jm-castle-warehouse-types": "^0.14.6",
    "jm-castle-types": "^0.1.1",
    "luxon": "^3.2.1"
  },
  "devDependencies": {
    "@types/luxon": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
